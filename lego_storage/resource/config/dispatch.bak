                        <sdboType>select</sdboType>
                        <namespace>selectFile</namespace>
                        <permission>storage_getFileByManager</permission>
                        <parameters>
                                <parameter>
                                        <!-- 目录uuid -->
                                        <name>directory_uuid</name>
                                        <type>string</type>
                                        <format>^[0-9a-zA-Z]{32}$</format>
                                        <isNull>true</isNull>
                                </parameter>
                                <parameter>
                                        <!-- 名称（1-32个汉字、字符、数字、下划线或横线） -->
                                        <name>name</name>
                                        <type>string</type>
                                        <format>^[\u4e00-\u9fffa0-9a-zA-Z_-]{1,32}$</format>
                                        <isNull>true</isNull>
